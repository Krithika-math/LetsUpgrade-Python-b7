      "source": [
        "for n in range(1042000, 702648265):\n",
        "  l = len(str(n))\n",
        "  sum = 0\n",
        "  temp = n\n",
        "  \n",
        "  while temp > 0:\n",
        "    d = temp % 10\n",
        "    sum += d ** l\n",
        "    temp //= 10\n",
        "\n",
        "  if n == sum:\n",
        "    print(n)\n",
        "    break"
      ],
